{
  "name": "codefinity-chat-mono",
  "private": true,
  "version": "0.0.1",
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "concurrently -n server,client -c red,blue \"pnpm --filter ./server dev\" \"pnpm --filter ./client dev\"",
    "build": "pnpm --filter ./server build && pnpm --filter ./client build",
    "start": "concurrently -n server,client -c red,blue \"pnpm --filter ./server start\" \"pnpm --filter ./client preview -- --host 0.0.0.0 --port 5173\""
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
